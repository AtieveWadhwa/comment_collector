{
    "manifest_version": 2,
    "name": "post collector",
    "version": "1.1",
    "permissions": [
        "https://www.haters.wtf/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.reddit.com/r/*","https://www.haters.wtf/*"],
            "js": ["content.js"]
        }
    ]
    
}